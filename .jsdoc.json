{
  "source": {
    "include": [
      "./",
      "README.md"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/docdash",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "default": {
      "includeDate": false
    },
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "plugins": []
}